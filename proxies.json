{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "root": {
      "matchCondition": {
        "route": "/",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/index.html"
    },
    "js": {
      "matchCondition": {
        "route": "/{filename}.js",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/{filename}.js"
    },
    "css": {
      "matchCondition": {
        "route": "/{filename}.css",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/{filename}.css"
    }
  }
}