{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "root": {
      "matchCondition": {
        "route": "/",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/index.html"
    },
    "ng": {
      "matchCondition": {
        "route": "/{*filename}",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/{filename}"
    },
    "api": {
      "matchCondition": {
        "route": "/api/{*all}",
        "methods": ["GET"]
      },
      "debug": true,
      "backendUri": "https://language-pronunciation.azurewebsites.net/api/{all}"
    }
  }
}