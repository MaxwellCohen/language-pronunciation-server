{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "proxy1": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/{*restOfPath}"
            },
            "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/{*restOfPath}"
        },
        "proxy2": {
            "matchCondition": {
                "methods": [ "GET" ],
                "route": "/"
            },
            "backendUri": "https://languagepronunciation.blob.core.windows.net/languagepronunciationwebpage/index.html"
        }
    }
}